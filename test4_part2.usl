public static void main(String[] args) {
    System.out.println("=== SISTEMA DE GESTION DE NOTAS INDIVIDUAL ===");
    
    String estudiante1 = "Ana";
    String estudiante2 = "Luis"; 
    String estudiante3 = "Maria";
    String estudiante4 = "Carlos";
    String estudiante5 = "Sofia";
    
    int nota1 = 85;
    int nota2 = 92;
    int nota3 = 78;
    int nota4 = 95;
    int nota5 = 88;
    
    int edad1 = 20;
    int edad2 = 19;
    int edad3 = 21;
    int edad4 = 20;
    int edad5 = 22;
    
    System.out.println("Procesando 5 estudiantes individuales");
    
    // IF / IF ELSE / ELSE / ELSE IF
    System.out.println("\n--- EVALUACION CON IF/ELSE ---");
    
    // Evaluacion estudiante 1
    System.out.println("Evaluando: " + estudiante1);
    if (nota1 >= 95) {
        System.out.println("Clasificacion: SOBRESALIENTE");
