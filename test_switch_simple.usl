public static void main(String[] args) {
    System.out.println("Test SWITCH simple:");
    int valor = 2;
    switch (valor) {
        case 1:
            System.out.println("Caso 1");
            break;
        case 2:
            System.out.println("Caso 2");
            break;
        default:
            System.out.println("Default");
            break;
    }
}

