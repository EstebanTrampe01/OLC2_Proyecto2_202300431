public static void main(String[] args) {
    System.out.println("Test contexto test3.usl:");
    
    // Simular FORs anteriores como en test3.usl
    for (int i = 1; i <= 5; i++) {
        System.out.println("FOR anterior - i: " + String.valueOf(i));
    }
    
    for (int i = 5; i >= 1; i--) {
        System.out.println("FOR anterior 2 - i: " + String.valueOf(i));
    }
    
    // Ahora la tabla de multiplicar
    System.out.println("Tabla de multiplicar:");
    for (int i = 1; i <= 3; i++) {
        for (int j = 1; j <= 4; j++) {
            int resultado = i * j;
            System.out.println(String.valueOf(i) + " x " + String.valueOf(j) + " = " + String.valueOf(resultado));
        }
    }
}
