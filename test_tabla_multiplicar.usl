public static void main(String[] args) {
    System.out.println("Test tabla de multiplicar:");
    
    for (int i = 1; i <= 3; i++) {
        for (int j = 1; j <= 4; j++) {
            int resultado = i * j;
            System.out.println(String.valueOf(i) + " x " + String.valueOf(j) + " = " + String.valueOf(resultado));
        }
    }
}
