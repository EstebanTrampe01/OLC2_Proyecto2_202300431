public static void main(String[] args) {
    System.out.println("Test FOR secuencial:");
    
    // Primer FOR
    for (int i = 1; i <= 2; i++) {
        System.out.println("FOR 1 - i: " + String.valueOf(i));
    }
    
    System.out.println("Entre FORs");
    
    // Segundo FOR
    for (int i = 3; i <= 4; i++) {
        System.out.println("FOR 2 - i: " + String.valueOf(i));
    }
}
