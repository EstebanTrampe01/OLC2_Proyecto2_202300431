public static void main(String[] args) {
    System.out.println("Test FOR m√∫ltiples:");
    
    // Primer FOR
    for (int i = 1; i <= 3; i++) {
        System.out.println("FOR 1 - i: " + String.valueOf(i));
    }
    
    // Segundo FOR (misma variable i)
    for (int i = 5; i >= 1; i--) {
        System.out.println("FOR 2 - i: " + String.valueOf(i));
    }
    
    // Tercer FOR con tabla de multiplicar
    for (int i = 1; i <= 3; i++) {
        for (int j = 1; j <= 4; j++) {
            int resultado = i * j;
            System.out.println(String.valueOf(i) + " x " + String.valueOf(j) + " = " + String.valueOf(resultado));
        }
    }
}
