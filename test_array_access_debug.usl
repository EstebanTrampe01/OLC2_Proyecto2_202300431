public static void main(String[] args) {
    System.out.println("Test Array Access Debug:");
    int[] notas = {85, 92, 78};
    
    // Test acceso directo
    int primera = notas[0];
    System.out.println("Primera nota: " + String.valueOf(primera));
    
    // Test acceso con variable
    int indice = 1;
    int segunda = notas[indice];
    System.out.println("Segunda nota: " + String.valueOf(segunda));
}
